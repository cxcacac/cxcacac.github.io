                <span class="k">return</span> <span class="kc">false</span><span class="o">;</span>
            <span class="o">}</span>
        <span class="o">}</span>
        <span class="k">return</span> <span class="n">count</span> <span class="o">==</span> <span class="mi">1</span><span class="o">;</span> <span class="c1">//æ˜¯å¦åªæœ‰ä¸€ä¸ªä¸åŒï¼Œä¸å¯ä»¥ç›¸åŒï¼Œä¸»è¦æ˜¯ä¸è‡ªèº«é‡åˆã€‚</span>
    <span class="o">}</span>
<span class="o">}</span>
